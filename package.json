{
  "name": "copy-line-number",
  "main": "./lib/copy-line-number",
  "version": "0.0.0",
  "description": "Save clipboard file name and line number.",
  "keywords": [
  ],
  "activationCommands": {
    "atom-workspace": "copy-line-number:copy"
  },
  "repository": "https://github.com/atom/copy-line-number",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
  },
  "configSchema": {
    "separateFormat": {
      "type": "string",
      "description": "Format to copy to clipboard. Replace to {1}=>FileName,{2}=>LineNumber",
      "default": "{1}:{2}",
      "order": 1
    }
  }
}
